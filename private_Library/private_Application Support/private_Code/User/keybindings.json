[
    ////////////////////////////////
    // GENERAL shortcuts
    ////////////////////////////////
    // remap caps_lock to escape in editor
    { "key": "ctrl+space", "command": "workbench.action.showCommands" },

    // maximise the screen real estate
    { "key": "ctrl+z", "command": "workbench.action.toggleZenMode" },

    // panel shortcuts
    { "key": "ctrl+shift+j", "command": "workbench.action.togglePanel" },
    { "key": "ctrl+shift+y", "command": "workbench.action.terminal.focus", "when": "panelVisible" },
    { "key": "ctrl+shift+u", "command": "workbench.action.problems.focus", "when": "panelVisible" },
    { "key": "ctrl+shift+i", "command": "workbench.panel.output.focus", "when": "panelVisible" },
    { "key": "ctrl+shift+o", "command": "workbench.action.debug.nextConsole", "when": "panelVisible" },
    { "key": "ctrl+shift+f", "command": "workbench.panel.testResults.view.focus", "when": "panelVisible" },
    { "key": "ctrl+shift+e", "command": "-cursorLineEndSelect", "when": "textInputFocus" },
    { "key": "ctrl+shift+e", "command": "workbench.explorer.fileView.toggleVisibility" },
    { "key": "ctrl+shift+x", "command": "workbench.views.extensions.marketplace.toggleVisibility" },
    { "key": "ctrl+shift+c", "command": "workbench.panel.chat.view.copilot.toggleVisibility" }

    { "key": "ctrl+[", "command": "notebook.diff.action.next", "when": "activeEditor == 'workbench.editor.notebookTextDiffEditor'" },
    { "key": "ctrl+]", "command": "notebook.diff.action.previous", "when": "activeEditor == 'workbench.editor.notebookTextDiffEditor'" },
    { "key": "ctrl+[", "command": "editor.action.dirtydiff.next", "when": "editorTextFocus && !textCompareEditorActive" },
    { "key": "ctrl+]", "command": "editor.action.dirtydiff.previous", "when": "editorTextFocus && !textCompareEditorActive" },
    { "key": "ctrl+[", "command": "workbench.action.editor.previousChange", "when": "editorTextFocus && !textCompareEditorActive" },
    { "key": "ctrl+]", "command": "workbench.action.editor.nextChange", "when": "editorTextFocus && !textCompareEditorActive" },

    // Toggle Testing View
    { "key": "ctrl+shift+=", "command": "workbench.view.testing.focus" },
    { "key": "ctrl+shift+=", "command": "workbench.action.toggleSidebarVisibility", "when": "sideBarFocus && sideBarVisible" },

    // scrolling in terminal
    { "key": "ctrl+shift+alt+j", "command": "workbench.action.terminal.scrollDownPage", "when": "terminalFocusInAny && terminalHasBeenCreated && !terminalAltBufferActive || terminalFocusInAny && terminalProcessSupported && !terminalAltBufferActive" },
    { "key": "ctrl+shift+alt+k", "command": "workbench.action.terminal.scrollUpPage", "when": "terminalFocusInAny && terminalHasBeenCreated && !terminalAltBufferActive || terminalFocusInAny && terminalProcessSupported && !terminalAltBufferActive" },
    // debugger
    { "key": "ctrl+shift+d", "command": "workbench.view.debug", "when": "viewContainer.workbench.view.debug.enabled" },
    { "key": "ctrl+shift+d", "command": "workbench.action.toggleSidebarVisibility", "when": "sideBarFocus && sideBarVisible" },
    // maximise a split (aka group)
    { "key": "ctrl+shift+k", "command": "workbench.action.toggleMaximizeEditorGroup", "when": "!terminalFocus && !panelFocus && editorFocus && (editorPartMaximizedEditorGroup || editorPartMultipleEditorGroups)" },
    // wrap lines in terminal to view more info
    { "key": "ctrl+shift+k", "command": "workbench.action.terminal.sizeToContentWidth", "when": "terminalFocus && terminalIsOpen" },
    // sidepanel focus
    { "key": "ctrl+shift+n", "command": "workbench.action.toggleSidebarVisibility", "when": "(!sideBarFocus && !sideBarVisible) || (sideBarFocus && sideBarVisible" },
    // todo-tree focus
    { "key": "ctrl+shift+t", "command": "todo-tree-view.focus" },
    // V4p
    { "key": "ctrl+shift+h", "command": "vide-v4p-vhdl-hierarchy.focus" },
    // formatting
    { "key": "ctrl+shift+p", "command": "editor.action.formatDocument", "when": "editorHasDocumentFormattingProvider && editorTextFocus" },

    // Unittesting
    { "key": "ctrl+shift+y", "command": "testing.runAll", "when": "sideBarFocus && sideBarVisible && workbench.view.testing.visible" },
    { "key": "ctrl+shift+u", "command": "testing.runAtCursor", "when": "!inDebugMode && editorTextFocus && !panelVisible" },

    // Debugging Shortcuts
    { "key": "ctrl+shift+y", "command": "workbench.action.debug.start", "when": "!inDebugMode && view == debug" },
    { "key": "ctrl+shift+y", "command": "workbench.action.debug.continue", "when": "inDebugMode" },
    { "key": "ctrl+shift+u", "command": "workbench.action.debug.stepOver", "when": "inDebugMode" },
    { "key": "ctrl+shift+i", "command": "workbench.action.debug.stepInto", "when": "inDebugMode" },
    { "key": "ctrl+shift+o", "command": "workbench.action.debug.stepOut", "when": "inDebugMode" },
    { "key": "ctrl+shift+p", "command": "workbench.action.debug.restart", "when": "inDebugMode" },
    { "key": "ctrl+shift+t", "command": "workbench.action.debug.stop", "when": "inDebugMode" },

    // complitions
    { "key": "ctrl+l", "command": "editor.action.inlineSuggest.acceptNextWord", "when": "inlineSuggestionVisible && !editorReadonly" },
    { "key": "ctrl+l", "command": "editor.action.accessibleViewAcceptInlineCompletion", "when": "accessibleViewIsShown && accessibleViewCurrentProviderId == 'inlineCompletions'" },

    // windows nav
    { "key": "ctrl+w q", "command": "workbench.action.closeActiveEditor" },
    { "key": "ctrl+w =", "command": "workbench.action.evenEditorWidths", "when": "editorTextFocus && vim.active && vim.mode == 'Normal'" },
    { "key": "ctrl+w v", "command": "workbench.action.splitEditor",      "when": "editorTextFocus && vim.active && vim.mode == 'Normal'" },
    { "key": "ctrl+w s", "command": "workbench.action.splitEditorDown",  "when": "editorTextFocus && vim.active && vim.mode == 'Normal'" },
    { "key": "ctrl+w l", "command": "workbench.action.focusRightGroup",  "when": "editorTextFocus && vim.active && vim.mode == 'Normal'" },
    { "key": "ctrl+w h", "command": "workbench.action.focusLeftGroup",   "when": "editorTextFocus && vim.active && vim.mode == 'Normal'" },
    { "key": "ctrl+w k", "command": "workbench.action.focusAboveGroup",  "when": "editorTextFocus && vim.active && vim.mode == 'Normal'" },
    { "key": "ctrl+w v", "command": "workbench.action.terminal.splitActiveTab", "when": "terminalProcessSupported && terminalTabsFocus" },
    { "key": "ctrl+w l", "command": "workbench.action.terminal.focusPreviousPane", "when": "terminalFocus && terminalHasBeenCreated || terminalFocus && terminalProcessSupported" },
    { "key": "ctrl+w h", "command": "workbench.action.terminal.focusNextPane",     "when": "terminalFocus && terminalHasBeenCreated || terminalFocus && terminalProcessSupported" },
    { "key": "alt+l", "command": "workbench.action.decreaseViewSize",    "when": "editorFocus || sideBarFocus" },
    { "key": "alt+h", "command": "workbench.action.increaseViewSize",    "when": "editorFocus || sideBarFocus" },
    { "key": "alt+j", "command": "workbench.action.decreaseViewSize",    "when": "panelFocus || terminalFocus" },
    { "key": "alt+k", "command": "workbench.action.increaseViewSize",    "when": "panelFocus || terminalFocus" },

    //find of all types
    { "key": "/", "command": "list.find", "when": "listFocus && listSupportsFind" },
    { "key": "/", "command": "settings.action.search", "when": "inSettingsEditor" },
    { "key": "/", "command": "editor.action.extensioneditor.showfind", "when": "!editorFocus && activeEditor == 'workbench.editor.extension'" },
    { "key": "/", "command": "keybindings.editor.searchKeybindings", "when": "inKeybindings" },

    // General Dropdowns
    { "key": "ctrl+j", "command": "list.focusDown", "when": "listFocus" },
    { "key": "ctrl+k", "command": "list.focusUp", "when": "listFocus" },

    // Command Palette
    { "key": "ctrl+j", "command": "editor.action.marker.next", "when": "editorFocus" },
    { "key": "ctrl+k", "command": "editor.action.marker.prev", "when": "editorFocus" },

    // IntelliSense
    { "key": "ctrl+j", "command": "selectNextSuggestion", "when": "suggestWidgetVisible" },
    { "key": "ctrl+k", "command": "selectPrevSuggestion", "when": "suggestWidgetVisible" },

    // Lightbulbs
    { "key": "ctrl+j", "command": "selectNextCodeAction", "when": "codeActionMenuVisible" },
    { "key": "ctrl+k", "command": "selectPrevCodeAction", "when": "codeActionMenuVisible" },

    // Problem Panel
    { "key": "ctrl+j", "command": "problems.action.focusProblems", "when": "panelFocus" },
    { "key": "ctrl+k", "command": "problems.action.focusProblems", "when": "panelFocus" },

    // File Explorer
    { "key": "ctrl+j", "command": "list.focusDown", "when": "explorerViewletVisible && filesExplorerFocus" },
    { "key": "ctrl+k", "command": "list.focusUp", "when": "explorerViewletVisible && filesExplorerFocus" },

    // Debug Console
    { "key": "ctrl+j", "command": "workbench.debug.panel.action.focusNext", "when": "inDebugMode" },
    { "key": "ctrl+k", "command": "workbench.debug.panel.action.focusPrevious", "when": "inDebugMode" },

    // Terminal Focus
    { "key": "ctrl+j", "command": "workbench.action.terminal.focusNext", "when": "terminalFocus" },
    { "key": "ctrl+k", "command": "workbench.action.terminal.focusPrevious", "when": "terminalFocus" },

    ////////////////////////////////
    // VIM-STYLE LISTS NAVIGATION
    ////////////////////////////////
    { "key": "ctrl+c", "command": "workbench.action.terminal.sendSequence", "args": { "text": "\u0003" }, "when": "terminalFocus" },
    { "key": "ctrl+`", "command": "editor.action.openLink" , "when": "editorTextFocus && vim.active && vim.mode == 'Normal'" },

    // sideBar
    { "key": "ctrl+h", "command": "list.collapse", "when": "sideBarFocus && !textInputFocus" },
    { "key": "ctrl+l", "command": "list.expand", "when": "sideBarFocus && !textInputFocus" },

    // Complex UI Widget Navigation (example: Settings UI, Command Palette)
    // { "key": "ctrl+j", "command": "widgetNavigation.focusNext", "when": "inputFocus && navigableContainerFocused || navigableContainerFocused && !listFocus || navigableContainerFocused && listScrollAtBoundary == 'both' || navigableContainerFocused && listScrollAtBoundary == 'bottom'" },
    // { "key": "ctrl+k", "command": "widgetNavigation.focusPrevious", "when": "inputFocus && navigableContainerFocused || navigableContainerFocused && !listFocus || navigableContainerFocused && listScrollAtBoundary == 'both' || navigableContainerFocused && listScrollAtBoundary == 'top'" },

    // Compare editor & Source Control NAVIGATION
    { "key": "ctrl+i w", "command": "toggle.diff.ignoreTrimWhitespace", "when": "textCompareEditorVisible && !sideBarFocus" },

    // GIT
    { "key": "ctrl+shift+g", "command": "workbench.view.scm", "when": "workbench.scm.active" },
    { "key": "ctrl+]", "command": "workbench.action.editor.nextChange", "when": "editorTextFocus" },
    { "key": "ctrl+[", "command": "workbench.action.editor.previousChange", "when": "editorTextFocus" },

    // Markdown Preview
    { "key": "ctrl+shift+m ctrl+shift+m", "command": "markdown.showPreview", "when": "editorTextFocus" },
    { "key": "ctrl+shift+alt+m", "command": "markdown.extension.closePreview", "when": "!editorTextFocus" },
    { "key": "ctrl+shift+m ctrl+shift+v", "command": "markdown.showPreviewToSide", "when": "editorTextFocus && vim.active" },

    // FILE TREE
    { "key": "s", "command": "explorer.openAndPassFocus", "when": "explorerViewletFocus && foldersViewVisible && !inputFocus" },
    { "key": "ctrl+s", "command": "explorer.openToSide", "when": "explorerViewletFocus && foldersViewVisible && !inputFocus" },
    { "key": "n", "command": "explorer.newFile", "when": "filesExplorerFocus && !inputFocus" },
    { "key": "f", "command": "explorer.newFolder", "when": "filesExplorerFocus && !inputFocus" },
    { "key": "d", "command": "deleteFile", "when": "filesExplorerFocus && !inputFocus" },
    { "key": "r", "command": "renameFile", "when": "filesExplorerFocus && !inputFocus" },

